{
  "name": "cepheus-onl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "clear && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "pretty": "prettier --write '**/*.*'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "nuke": "rm -rf .next node_modules package-lock.json && npm install && npm run dev",
    "verify": "prettier --write . && next lint --fix --no-cache && npx tsc --noEmit",
    "check": "npm run verify && npm run test:e2e",
    "deps": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm run nuke",
    "migrate:local": "wrangler d1 migrations apply cepheus-onl-db --local"
  },
  "dependencies": {
    "@dimforge/rapier3d": "^0.17.1",
    "@dimforge/rapier3d-compat": "^0.17.1",
    "@google-cloud/text-to-speech": "^6.1.0",
    "@google/genai": "^1.0.1",
    "@react-three/drei": "^10.0.8",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@react-three/rapier": "^2.1.0",
    "better-sqlite3": "^11.10.0",
    "fast-equals": "^5.2.2",
    "immer": "^10.1.1",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "simplex-noise": "^4.0.3",
    "three": "^0.176.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.28",
    "zustand": "^5.0.5",
    "howler": "^2.2.4"
  },
  "overrides": {
    "@xstate/react": "5.0.5",
    "xstate": "^5.19.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@flydotio/dockerfile": "^0.7.10",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.23.0",
    "drizzle-orm": "^0.32.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "npm-check-updates": "^18.0.1",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.32.1",
    "wrangler": "^3.67.1"
  }
}
