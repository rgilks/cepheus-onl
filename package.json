{
  "name": "cepheus-onl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "clear && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "pretty": "prettier --write '**/*.*'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "nuke": "rm -rf .next node_modules package-lock.json && npm install && npm run dev",
    "verify": "prettier --write . && next lint --fix --no-cache && npx tsc --noEmit",
    "check": "npm run verify && npm run test:e2e",
    "deps": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm run nuke"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "^1.6.3",
    "@aws-amplify/ui-react": "react-19",
    "aws-amplify": "^6.15.0",
    "dotenv": "^16.5.0",
    "next": "15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@react-spring/three": "^10.0.1",
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^10.1.2",
    "@react-three/fiber": "^9.1.2",
    "immer": "^10.1.1",
    "three": "^0.177.0",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.7.2",
    "@playwright/test": "^1.52.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "aws-cdk": "^2.1018.0",
    "aws-cdk-lib": "^2.200.1",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.5",
    "eslint": "^9",
    "aws-sdk": "^2.1692.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "tailwindcss": "^4",
    "prettier": "^3.5.3",
    "npm-check-updates": "^18.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
